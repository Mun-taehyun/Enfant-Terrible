<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.enfantTerrible.enfantTerrible.mapper.product.ProductSkuOptionValueQueryMapper">

  <select id="findOptionValueIdsBySkuId" resultType="long">
    SELECT option_value_id
    FROM et_product_sku_option
    WHERE sku_id = #{skuId}
    ORDER BY option_value_id ASC
  </select>

</mapper>
